<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.bbs.android.log">
    <uses-sdk android:targetSdkVersion="23" android:minSdkVersion="4"
        tools:overrideLibrary="android.support.v7.appcompat"/>
    <application>
        <activity android:name="org.bbs.android.log.Logcat_FragmentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="org.bbs.android.log.Logcat_AppCompatActivity"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
